{
    "name": "knights-of-darkness-browser",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "build:types": "tsc -b && vite build",
        "build:watch": "tsc -b --watch",
        "lint": "eslint \"src/**/*.{js,mjs,cjs,ts,jsx,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{js,mjs,cjs,ts,jsx,tsx}\" --fix",
        "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "ci:lint": "npm run lint && npm run format:check",
        "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "preview": "vite preview"
    },
    "dependencies": {
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@mui/icons-material": "^6.1.6",
        "@mui/material": "^6.1.6",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.28.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.23.0",
        "@types/babel__generator": "^7.6.8",
        "@types/babel__template": "^7.4.4",
        "@types/babel__traverse": "^7.20.6",
        "@types/prop-types": "^15.7.13",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.23.0",
        "eslint-import-resolver-typescript": "^4.2.3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "prettier": "^3.5.3",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.28.0",
        "vite": "^5.4.15"
    }
}
